    <section class="u-clearfix u-grey-10 u-section-1" id="sec-af78">
      <div class="u-clearfix u-sheet u-sheet-1">
        <h2 class="u-align-center u-text u-text-1">WITHDRAWAL&nbsp;<b>REPORT</b>
          <br>
        </h2>
        <h4 class="u-align-left u-text u-text-2">Hostel : <span id = "hostelName" ><%= details.hostelName %></h4>
        <h4 class="u-align-left u-text u-text-3">Room Number : <span  id = "roomNumber"> <%= details.roomNumber %></h4>
        <h4 class="u-align-left u-text u-text-4">Student Details&nbsp;</h4>
        

          <div class="u-expanded-width u-table u-table-responsive u-table-1">
            <table class="u-table-entity">
              <colgroup>
                <col width="16.5%">
                <col width="16.5%">
                <col width="16.5%">
                <col width="16.5%">
                <col width="16.5%">
                <col width="16.5%">
              </colgroup>
              <thead class="u-black u-table-header u-table-header-1">
                <tr style="height: 48px;">
                  <th class="u-table-cell">NAME</th>
                  <th class="u-table-cell">REG NO</th>
                  <th class="u-table-cell">FACULTY</th>
                  <th class="u-table-cell">YEAR</th>
                  <th class="u-table-cell">ISSUED DATE</th>
                  <th class="u-table-cell">RECEIVED DATE</th>

                </tr>
              </thead>
              <tbody class="u-table-alt-palette-5-light-1 u-table-body">
                <% details.roomDetails.forEach( data => { %>
                  <tr style="height: 67px;">
    
                    <td class="u-table-cell"><%= data.student_name %></td>
                    <td class="u-table-cell"><%= data.reg_no %></td>
                    <td class="u-table-cell"><%= data.faculty %></td>
                    <td class="u-table-cell"><%= data.year %></td>
                    <td class="u-table-cell"><%= data.issued_date.toISOString().split('T')[0] %></td>
                    <td class="u-table-cell"><%= data.received_date.toISOString().split('T')[0]%></td>
    
                  </tr>
                  <% }) %>
              </tbody>
            </table>
          </div>

        <h4 class="u-align-left u-text u-text-4">Item Details&nbsp;</h4>

        <div class="u-expanded-width u-table u-table-responsive u-table-1">
          <table class="u-table-entity">
            <colgroup>
              <col width="33.1%">
              <col width="33.1%">
              <col width="33.8%">
            </colgroup>
            <thead class="u-black u-table-header u-table-header-1">
              <tr style="height: 48px;">
                <th class="u-table-cell">ITEM</th>
                <th class="u-table-cell">REASON</th>
                <th class="u-table-cell">FINE</th>
              </tr>
            </thead>
            <tbody class="u-table-alt-palette-5-light-1 u-table-body">
              <% tableData.forEach( data => { %>
                <tr style="height: 67px;">
  
                  <td class="u-table-cell"><%= data.name %></td>
                  <td class="u-table-cell">Missing Item</td>
                  <td class="u-table-cell"><%= data.cost %></td>
  
                </tr>
                <% }) %>
            </tbody>
            <tfoot class="u-palette-1-light-2 u-table-footer u-table-footer-1">
              <tr style="height: 45px;">
                <td class="u-border-4 u-border-grey-75 u-border-no-left u-border-no-right u-table-cell u-table-cell-25">TOTAL PAYABLE</td>
                <td class="u-border-4 u-border-grey-75 u-border-no-left u-border-no-right u-table-cell u-table-cell-26"></td>
                <td class="u-border-4 u-border-grey-75 u-border-no-left u-border-no-right u-table-cell u-table-cell-27"> <%= totalCost %></td>
              </tr>
            </tfoot>
          </table>
        </div>
        <a href="/WITHDRAW-ROOMS" class="u-active-custom-color-3 u-btn u-btn-round u-button-style u-grey-90 u-hover-custom-color-2 u-radius-50 u-btn-1">PRINT WITHDRAWAL REPORT</a>
      </div>
